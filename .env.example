# arch-layer-prod-mongo-fast Configuration
# Copy to .env and fill in values

# Application
APP_TITLE=Layered Architecture Demo
APP_VERSION=0.1.0
APP_DEBUG=true

# MongoDB
MONGODB_URI=mongodb://localhost:27017
